<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ivRPS"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RPSActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0.2"
        app:srcCompat="@drawable/background"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <View
            android:id="@+id/rectangleView"
            android:layout_width="159dp"
            android:layout_height="159dp"
            android:alpha="1"
            android:background="@drawable/rounded_rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.173"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.242" />

        <View
            android:id="@+id/rectangleView2"
            android:layout_width="159dp"
            android:layout_height="159dp"
            android:background="@drawable/rounded_rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.753"
            app:layout_constraintStart_toEndOf="@+id/rectangleView"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.242" />

        <View
            android:id="@+id/imageView99"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:background="@drawable/rounded_rectangle2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.276"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.873" />

        <View
            android:id="@+id/imageView98"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:background="@drawable/rounded_rectangle2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.513"
            app:layout_constraintStart_toEndOf="@+id/imageView99"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.873" />

        <View
            android:id="@+id/imageView97"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:background="@drawable/rounded_rectangle2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView98"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toEndOf="@+id/imageView99"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.873" />

        <TextView
            android:id="@+id/tvHumanScore"
            android:layout_width="71dp"
            android:layout_height="86dp"
            android:text="0"
            android:textAlignment="center"
            android:textSize="60sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.389"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.316" />

        <TextView
            android:id="@+id/tvCompScore"
            android:layout_width="71dp"
            android:layout_height="86dp"
            android:text="0"
            android:textAlignment="center"
            android:textSize="60sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.601"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.314" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="87dp"
            android:layout_height="87dp"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tvCompScore"
            app:layout_constraintStart_toEndOf="@+id/tvHumanScore"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.315"
            app:srcCompat="@drawable/vs" />

        <TextView
            android:id="@+id/titleQueueNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rock Paper Scissors"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.115" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="148dp"
            android:layout_height="62dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/imgbin_nanyang_polytechnic_republic_polytechnic_school_education_diploma_png" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:layout_constraintBottom_toBottomOf="@+id/rectangleView"
            app:layout_constraintEnd_toEndOf="@+id/rectangleView"
            app:layout_constraintStart_toStartOf="@+id/rectangleView"
            app:layout_constraintTop_toTopOf="@+id/rectangleView"
            app:srcCompat="@drawable/user" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:layout_marginTop="23dp"
            app:layout_constraintBottom_toBottomOf="@+id/rectangleView2"
            app:layout_constraintEnd_toEndOf="@+id/rectangleView2"
            app:layout_constraintHorizontal_bias="0.53"
            app:layout_constraintStart_toStartOf="@+id/rectangleView2"
            app:layout_constraintTop_toTopOf="@+id/rectangleView2"
            app:layout_constraintVertical_bias="0.038"
            app:srcCompat="@drawable/dumpy_copy" />

        <ImageView
            android:id="@+id/ivRock"
            android:layout_width="80dp"
            android:layout_height="80dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView99"
            app:layout_constraintEnd_toEndOf="@+id/imageView99"
            app:layout_constraintStart_toStartOf="@+id/imageView99"
            app:layout_constraintTop_toTopOf="@+id/imageView99"
            app:srcCompat="@drawable/animation_computer_rock" />

        <ImageView
            android:id="@+id/ivPaper"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView97"
            app:layout_constraintEnd_toEndOf="@+id/ivScissors"
            app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintStart_toStartOf="@+id/imageView97"
            app:layout_constraintTop_toTopOf="@+id/imageView97"
            app:srcCompat="@drawable/animation_computer_paper" />

        <ImageView
            android:id="@+id/ivScissors"
            android:layout_width="80dp"
            android:layout_height="80dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView98"
            app:layout_constraintEnd_toEndOf="@+id/imageView98"
            app:layout_constraintStart_toStartOf="@+id/imageView98"
            app:layout_constraintTop_toTopOf="@+id/imageView98"
            app:srcCompat="@drawable/animation_computer_scissors" />

        <ImageView
            android:id="@+id/ivHome"
            android:layout_width="82dp"
            android:layout_height="60dp"
            app:layout_constraintBottom_toTopOf="@+id/rectangleView"
            app:layout_constraintEnd_toStartOf="@+id/titleQueueNum"
            app:layout_constraintHorizontal_bias="0.406"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.34"
            app:srcCompat="@drawable/home" />

        <ImageView
            android:id="@+id/ivBack"
            android:layout_width="82dp"
            android:layout_height="60dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toTopOf="@+id/rectangleView"
            app:layout_constraintEnd_toStartOf="@+id/ivHome"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/back" />

        <ImageView
            android:id="@+id/imageView26"
            android:layout_width="127dp"
            android:layout_height="127dp"
            android:src="@drawable/opaque_rounded_rectangle"
            app:layout_constraintBottom_toTopOf="@+id/imageView98"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.637"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rectangleView2" />

        <ImageView
            android:id="@+id/imageView27"
            android:layout_width="127dp"
            android:layout_height="127dp"
            android:src="@drawable/opaque_rounded_rectangle"
            app:layout_constraintBottom_toTopOf="@+id/imageView99"
            app:layout_constraintEnd_toStartOf="@+id/imageView26"
            app:layout_constraintHorizontal_bias="0.716"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rectangleView" />

        <ImageView
            android:id="@+id/iv_HumanChoice"
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27"
            app:layout_constraintEnd_toEndOf="@+id/imageView27"
            app:layout_constraintStart_toStartOf="@+id/imageView27"
            app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:srcCompat="@drawable/opaque_rounded_rectangle" />

        <ImageView
            android:id="@+id/iv_CompChoice"
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26"
            app:layout_constraintEnd_toEndOf="@+id/imageView26"
            app:layout_constraintStart_toStartOf="@+id/imageView26"
            app:layout_constraintTop_toTopOf="@+id/imageView26"
            app:srcCompat="@drawable/opaque_rounded_rectangle" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>